<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#7BA4C5"
    android:backgroundTint="#FAF8F8"
    android:scrollbarSize="10dp"
    tools:context=".MainActivity">


    <ListView
        android:id="@+id/listSismos"
        android:layout_width="345dp"
        android:layout_height="536dp"
        android:backgroundTint="#45B6C5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.358" />

    <Button
        android:id="@+id/boton_recargar"
        style="@style/Widget.Material3.Button"
        android:layout_width="149dp"
        android:layout_height="57dp"
        android:backgroundTint="#4DCDC1"
        android:drawableTop="@color/design_default_color_primary_dark"
        android:onClick="Actualizar"
        android:scrollbarTrackVertical="@drawable/ic_launcher_background"
        android:text=""
        android:textColor="#FAF4F4"
        android:textSize="20sp"
        android:textAlignment="center"
        android:layout_gravity="center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listSismos"
        app:layout_constraintVertical_bias="0.617" />

    <TextView
        android:id="@+id/textView"
        android:text=""
        android:layout_width="420dp"
        android:layout_height="54dp"
        android:background="#76DCD2"
        app:layout_constraintBottom_toTopOf="@+id/listSismos"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX=""
        android:backgroundTint="#69C5BC"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="217dp"
        android:layout_height="28dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Teluricap"
        android:textAlignment="center"
        android:textColor="#FDF6F6"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="103dp"
        android:layout_height="31dp"
        android:text="Actualizar"
        android:fontFamily="sans-serif-condensed-medium"
        android:textColor="#FAF4F4"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/boton_recargar"
        app:layout_constraintEnd_toEndOf="@+id/boton_recargar"
        app:layout_constraintHorizontal_bias="0.63"
        app:layout_constraintStart_toStartOf="@+id/boton_recargar"
        app:layout_constraintTop_toTopOf="@+id/boton_recargar" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="194dp"
        android:layout_height="26dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="SismologÃ­a Chile"
        android:textAlignment="center"
        android:textColor="#FDF6F6"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


</androidx.constraintlayout.widget.ConstraintLayout>